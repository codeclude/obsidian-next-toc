{
	"name": "next-toc",
	"version": "2.0.0",
	"description": "Floating panel displaying the current document's reading progress, table of contents, and navigation aids.",
	"main": "main.js",
	"scripts": {
		"dev": "node scripts/esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node scripts/esbuild.config.mjs production",
		"build:local": "tsc -noEmit -skipLibCheck && node scripts/esbuild.config.mjs production keep-console && node scripts/copy-to-vault.mjs",
		"version": "pnpm run build && node scripts/version-bump.mjs && pnpm run changelog:u && pnpm i",
		"changelog:u": "conventional-changelog -p angular -i CHANGELOG.md -s -u -n ./scripts/changelog-option.js",
		"changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -n ./scripts/changelog-option.js",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"i18n:typesafe": "typesafe-i18n",
		"i18n:sync": "node scripts/sync-i18n.mjs",
		"test": "jest",
		"test:watch": "jest --watch",
		"link:data": "node scripts/link-data.mjs"
	},
	"keywords": [
		"Obsidian Plugin",
		"Table of Contents",
		"Reading Progress"
	],
	"author": "RavenHogWarts",
	"license": "GPL-3.0-only",
	"engines": {
		"node": ">=18.x"
	},
	"pnpm": {
		"overrides": {
			"@conventional-changelog/git-client": "^2.0.0"
		}
	},
	"overrides": {
		"@conventional-changelog/git-client": "^2.0.0"
	},
	"devDependencies": {
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.38.6",
		"@types/jest": "^30.0.0",
		"@types/node": "^25.0.10",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"builtin-modules": "5.0.0",
		"conventional-changelog-cli": "^5.0.0",
		"dotenv": "^17.2.3",
		"esbuild": "0.27.2",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"fs-extra": "^11.3.3",
		"jest": "^30.2.0",
		"obsidian": "^1.11.4",
		"postcss": "^8.5.6",
		"postcss-nesting": "^14.0.0",
		"ts-jest": "^29.4.6",
		"tslib": "2.8.1",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"lucide-react": "^0.563.0",
		"radix-ui": "^1.4.3",
		"react": "^19.2.4",
		"react-circular-progressbar": "^2.2.0",
		"react-dom": "^19.2.4"
	}
}
